{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api"
        },
        {
            "source": "/(.*)",
            "destination": "/public/$1"
        }
    ],
    "functions": {
        "api/generate.js": {
            "runtime": "nodejs18.x"
        }
    }
}
